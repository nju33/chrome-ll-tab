{
  "manifest_version": 2,
  "name": "__MSG_appName__",
  "description": "__MSG_appDesc__",
  "version": "2.0.2",
  "default_locale": "en",
  "omnibox": {
    "keyword": "ll"
  },
  "permissions": [
    "storage",
    "tabs"
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "icons/icon19.png",
      "38": "icons/icon38.png"
    },
    "default_title": "ll",
    "default_popup": "popup/index.html"
  },
  "background": {
    "persistent": false,
    "scripts": ["scripts/background.js"]
  },
  "content_security_policy":"script-src 'self' 'unsafe-eval'; object-src 'self'"
}
